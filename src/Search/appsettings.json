{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Search.Services.NlWebNetClient": "Debug"
    }
  },
  "AllowedHosts": "*",
  "NLWeb": {
    "Endpoint": "http://nlweb:8000",
    "Timeout": "00:00:30",
    "RetryPolicy": {
      "MaxRetries": 3,
      "BaseDelay": "00:00:01"
    }
  },
  "Search": {
    "DefaultResultCount": 10,
    "MaxResultCount": 50,
    "MaxQueryLength": 1000
  }
}
